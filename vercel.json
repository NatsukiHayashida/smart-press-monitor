{
  "version": 2,
  "framework": "nextjs",
  "installCommand": "cd press-machine-web && npm install",
  "buildCommand": "cd press-machine-web && npm run build",
  "outputDirectory": "press-machine-web/.next",
  "functions": {
    "press-machine-web/.next/server/**/*.js": {
      "maxDuration": 10
    }
  },
  "regions": ["hnd1"],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@next_public_supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@next_public_supabase_anon_key"
  }
}